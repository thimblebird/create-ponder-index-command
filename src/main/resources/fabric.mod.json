{
  "schemaVersion": 1,
  "id": "cpic",
  "version": "${version}",
  "name": "Create: Ponder Index Command",
  "description": "Adds a client command (/cpic) to open the Ponder Index screen",
  "authors": [
    "thimblebird"
  ],
  "contact": {
    "sources": "https://github.com/thimblebird/create-ponder-index-command"
  },
  "license": "MIT",
  "icon": "assets/cpic/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "io.thimblebird.cpic.PonderIndexCommand"
    ]
  },
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": ">=${fabric_api_version}",
    "create": "${create_version}",
    "minecraft": "${minecraft_version}"
  }
}
